@article{Chalk,
	title={Eigenvector statistics in non-{H}ermitian random matrix ensembles},
	author={Chalker, J. T. and Mehlig, B.},
	journal={Physical Review Letters},
	volume={81},
	number={16},
	pages={3367--3370},
	year={2012},
	keywords={top quark;inclusive cross section;perturbative calculations},
	abstract={We study statistical properties of the eigenvectors of non-Hermitian random matrices, concentrating on Ginibre's complex Gaussian ensemble, in which the real and imaginary parts of each element of an N x N matrix, J, are independent random variables. Calculating ensemble averages based on the quantity $< L_\alpha | L_\beta > < R_\beta | R_\alpha >$, where $< L_\alpha |$ and $| R_\beta >$ are left and right eigenvectors of J, we show for large N that eigenvectors associated with a pair of eigenvalues are highly correlated if the two eigenvalues lie close in the complex plane. We examine consequences of these correlations that are likely to be important in physical applications.}
}

